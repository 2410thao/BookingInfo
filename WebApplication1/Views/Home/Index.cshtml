@{
    ViewData["Title"] = "Home Page";
}

@model List<string>

@section Scripts {
    <script src="~/js/scroll.js"></script>
}
<div id="header">
    <ul>
        <li class="brand">
            <div>
                <div id="container_icon">
                    <svg id="icon" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg"><g id="SVGRepo_bgCarrier" stroke-width="0"></g><g id="SVGRepo_tracerCarrier" stroke-linecap="round" stroke-linejoin="round"></g><g id="SVGRepo_iconCarrier"> <g clip-path="url(#clip0_429_11160)"> <circle cx="12" cy="11.9999" r="9" stroke="#ff0000" stroke-width="2.5" stroke-linecap="round" stroke-linejoin="round"></circle> <rect x="12" y="8" width="0.01" height="0.01" stroke="#ff0000" stroke-width="3.75" stroke-linejoin="round"></rect> <path d="M12 12V16" stroke="#ff0000" stroke-width="2.5" stroke-linecap="round" stroke-linejoin="round"></path> </g> <defs> <clipPath id="clip0_429_11160"> <rect width="24" height="24" fill="white"></rect> </clipPath> </defs> </g></svg>
                    <p class="title_header">Booking</p>
                </div>
                <div>
                </div>
            </div>
        </li>
        <li class="Search">
            <input id="inputSearchCus" class="search_infor_booking logo_search" placeholder="Tìm kiếm" />
            <button id="btnSearch" onclick="onSearch();">Tìm</button>
        </li>
    </ul>
</div>

<div id="GetInforBooking">
    @foreach (var item in Model)
    {
        @Html.Raw(item)
    }
</div>

<div id="footer_body">
    <div class="container_button_footer">
        <a asp-action="NewDetail" asp-controller="Home">
            <button id="btnCreateNew" class="padding_button" type="button">Tạo mới</button>
        </a>
    </div>
</div>

<script>
    function Create_infor_book(code) {
        const url = `/Home/NewDetail?code=${code}`;

        // Redirect the user to the new page
        window.location.href = url;
    }
</script>